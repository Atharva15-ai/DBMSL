create or replace procedure proce_grad(r in number,m in number,gr out varchar)
is
begin
if m<=1500 and m>=900 then
gr:='dst with dist';
elsif m<=989 and m>=900 then
gr:='ist class';
elsif m<=899 and m>=825 then
gr:='iind class';
else
gr:='fail';
end if;
end;
